spring.application.name=MicroserviceForHistory
server.port=8082

spring.datasource.url=jdbc:postgresql://localhost:5432/mediabuddy
spring.datasource.username=YOUR_USERNAME
spring.datasource.password=YOUR_PASS
spring.datasource.driver-class-name=org.postgresql.Driver

spring.jpa.database-platform=org.hibernate.dialect.PostgreSQLDialect
spring.jpa.show-sql=true

spring.jpa.hibernate.ddl-auto=update

spring.rabbitmq.host=localhost
spring.rabbitmq.port=5672
spring.rabbitmq.username=YOUR_ADMIN
spring.rabbitmq.password=YOUR_SECRET

spring.rabbitmq.stream.host=localhost
spring.rabbitmq.stream.port=5552
spring.rabbitmq.stream.username=YOUR_USERNAME
spring.rabbitmq.stream.password=YOUR_SECRET